schema{
    query: Query
}

type Query{
    getAllExam : [Exam],
    getExamById(examId: Int!): Exam
    getAllQuestions : [Question],
    getQuestionById(questionId: Int!): Question
}

type Exam{
  	  examId : Int
	  examName : String
	  description : String
	  minutes : Int,
	  questions : [Question]
}

type Question{
	questionId : Int
	question : String
	answer : String
}